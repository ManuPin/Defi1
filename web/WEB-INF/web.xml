<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AfficheProduit</servlet-name>
        <servlet-class>com.defi.servlet.AfficheProduit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetPanier</servlet-name>
        <servlet-class>com.defi.servlet.GetPanier</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>com.defi.servlet.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AfficheProduit1</servlet-name>
        <servlet-class>com.defi.servlet.AfficheProduit1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Commande</servlet-name>
        <servlet-class>com.defi.servlet.Commande</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResetSession</servlet-name>
        <servlet-class>com.defi.magasin.ResetSession</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AfficheProduit</servlet-name>
        <url-pattern>/AfficheProduit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetPanier</servlet-name>
        <url-pattern>/GetPanier</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AfficheProduit1</servlet-name>
        <url-pattern>/AfficheProduit1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Commande</servlet-name>
        <url-pattern>/Commande</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetSession</servlet-name>
        <url-pattern>/ResetSession</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
